{
  "name": "nutrikompass",
  "version": "0.1.0",
  "description": "KI-gestuetzte Ernaehrungsplanung fuer Einrichtungen mit Fokus auf Essstoerungen bei Jugendlichen.",
  "private": true,
  "license": "MIT",
  "author": "Lengi96",
  "repository": {
    "type": "git",
    "url": "https://github.com/Lengi96/Nutritionkompass.de.git"
  },
  "bugs": {
    "url": "https://github.com/Lengi96/Nutritionkompass.de/issues"
  },
  "homepage": "https://mein-nutrikompass.de",
  "keywords": [
    "nextjs",
    "trpc",
    "prisma",
    "nutrition",
    "healthcare",
    "meal-planning",
    "openai"
